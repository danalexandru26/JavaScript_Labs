function syntaxBlock() {
    let a = 10;
    let b = 20;

    var c = 10;
    var d = 20;

    let sum = a + b;
    var product = c * d;

    console.log("Sum: " + sum);
    console.log("Product: " + product);

    let myName = "Explorer";
    
    if(myName === "Explorer") {
        var greeting = "Welcome, Explorer!";
        console.log(greeting);
    }

    //The following line is a string literal
    "Bella";
    
    //The following line is a numeric literal
    10;
}